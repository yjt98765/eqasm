{
    "name": "eQASM",
    "scopeName": "text.eqasm",
    "fileTypes": [
        "eqasm"
    ],
    "patterns": [{
            "explanation": "Line continuation",
            "match": "\\s*\\\\s*$",
            "name": "constant.character.escape"
        },
        {
            "match": "(?i)(.register)\\s*([rR][0-9]?)\\s*([a-zA-Z0-9_]+)",
            "captures": {
                "1": {
                    "name": "markup.bold.eqasm"
                },
                "2": {
                    "name": "constant.numeric.integer.eqasm"
                },
                "3": {
                    "name": "string.unquoted,markup.italic"
                }
            }

        },
        {
            "match": "(?i)(.def_sym)\\s*([a-zA-Z0-9_]+)\\s*(0x[0-9a-f]+|0o[0-7]+|0b[01]+)",
            "captures": {
                "1": {
                    "name": "markup.bold.eqasm"
                },
                "2": {
                    "name": "string.unquoted,markup.italic"
                },
                "3": {
                    "name": "constant.numeric.integer.eqasm"
                }
            }

        },

        {
            "match": "(?i)(bs)\\s*([0-7]?)",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "constant.numeric.preinterval.eqasm"
                }
            }

        },

        {
            "match": "#.*",
            "name": "comment.line.number-sign"
        },
        {
            "match": "(?i)\\b(BR)\\s*(ALWAYS|NEVER|EQ|NE|EQZ|NEZ|LT|LTZ|LE|GT|GE|GEZ|LTU|LEU|GTU|GEU|CARRY|NOTCARRY)\\s*,\\s*([a-zA-Z0-9_]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "emphasis"
                },
                "3": {
                    "name": "markup.bold"
                }
            }
        },
        {
            "match": "(?i)\\b(BLTU|BLEU|BGTU|BGEU|BEQ|BNE|BLT|BLE|BGT|BGE)\\s*([a-zA-Z0-9]+)\\s*,\\s*([a-zA-Z0-9]+)\\s*,\\s*([a-zA-Z0-9_]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "variable.parameter.eqasm"
                },
                "3": {
                    "name": "variable.parameter.eqasm"
                },
                "4": {
                    "name": "markup.bold"
                }
            }
        },
        {
            "match": "(?i)\\b(LB|LW|SB|SW)\\s*([a-zA-Z0-9]+)\\s*,\\s*([a-zA-Z0-9]+)\\s*,\\s*([a-zA-Z0-9_]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "variable.parameter.eqasm"
                },
                "3": {
                    "name": "markup.bold"
                }
            }
        },
        {
            "match": "(?i)\\b(GOTO|BRA|BRN)\\s*([a-zA-Z0-9_]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "markup.bold"
                }
            }
        },

        {
            "match": "(?i)\\b(ADD|OR|SLT|SUBC|ADDC|SUB|OR|XOR|DIV|REM|MUL|AND|XNOR|NAND|NOR)\\s*([a-zA-Z0-9]+)\\s*,\\s*([a-zA-Z0-9_]+),\\s*([a-zA-Z0-9_]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "variable.parameter.eqasm"
                },
                "3": {
                    "name": "variable.parameter.eqasm"
                },
                "4": {
                    "name": "variable.parameter.eqasm"
                }
            }
        },
        {
            "match": "(?i)\\b(ADDI|SLTIU)\\s*([a-zA-Z0-9]+)\\s*,\\s*([a-zA-Z0-9_]+),\\s*([a-zA-Z0-9_]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "variable.parameter.eqasm"
                },
                "3": {
                    "name": "variable.parameter.eqasm"
                },
                "4": {
                    "name": "constant.numeric.integer.eqasm"
                }
            }
        },
        {
            "match": "(?i)\\b(NOT|CMP|LD|ST|SHL1|COPY|MOV|MULT2)\\s*([a-zA-Z0-9]+)\\s*,\\s*([a-zA-Z0-9_]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "variable.parameter.eqasm"
                },
                "3": {
                    "name": "variable.parameter.eqasm"
                }
            }
        },
        {
            "match": "(?i)\\b(LDI)\\s*([a-zA-Z0-9]+)\\s*,\\s*(0x[0-9a-f]+|0o[0-7]+|0b[01]+|[0-9]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "variable.parameter.eqasm"
                },
                "3": {
                    "name": "constant.numeric.integer.eqasm"
                }
            }
        },
        {
            "match": "(?i)\\b(LDUI)\\s*([a-zA-Z0-9]+)\\s*,\\s*(0x[0-9a-f]+|0o[0-7]+|0b[01]+|[0-9]+)\\s*,\\s*([a-zA-Z0-9_]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "variable.parameter.eqasm"
                },
                "3": {
                    "name": "constant.numeric.integer.eqasm"
                },
                "4": {
                    "name": "variable.parameter.eqasm"
                }
            }
        },
        {
            "match": "(?i)\\b(FMR)\\s*([a-zA-Z0-9]+)\\s*,\\s*([qQ][0-9]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "variable.parameter.eqasm"
                },
                "3": {
                    "name": "entity.name.type"
                }
            }
        },
        {
            "match": "(?i)\\b(FBR)\\s*([a-zA-Z0-9]+)\\s*,\\s*([a-zA-Z0-9]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "emphasis"
                },
                "3": {
                    "name": "variable.parameter.eqasm"
                }
            }
        },
        {
            "match": "(?i)\\b(QWAITR)\\s*([a-zA-Z0-9]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "variable.parameter.eqasm"
                }
            }
        },
        {
            "match": "(?i)\\b(QWAIT)\\s*([a-zA-Z0-9]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "constant.numeric.integer.eqasm"
                }
            }
        },
        {
            "match": "(?i)\\b(NOP|STOP|QNOP|CNOT)\\b",
            "name": "keyword.control.eqasm"
        },
        {
            "match": "(?i)\\b(SMIS|SMIT)\\s*([sStT][0-9]+)\\b",
            "captures": {
                "1": {
                    "name": "keyword.control.eqasm"
                },
                "2": {
                    "name": "entity.name.type"
                }
            }
        },
        {
            "match": "(?i)\\b([A-Za-z0-9_]+)\\s*([sStT][0-9]+)\\b",
            "captures": {
                "1": {
                    "name": "string.jade"
                },
                "2": {
                    "name": "entity.name.type"
                }
            }
        },
        {
            "match": "(?i)([A-Za-z0-9][A-Za-z0-9._-]*[A-Za-z0-9]|[A-Za-z0-9])(:)",
            "captures": {
                "1": {
                    "name": "markup.bold"
                },
                "2": {
                    "name": "comment.line.number-sign"
                }
            }
        },
        {
            "match": "(?i)\\b([sS][0-9]+)\\b",
            "name": "entity.name.type"
        },
        {
            "match": "(?i)\\b([tT][0-9]+)\\b",
            "name": "entity.name.type"

        },
        {
            "match": "(?i)\\b([qQ][0-9]+)\\b",
            "name": "entity.name.type"

        },
        {
            "match": "(?i)\\b(0x[0-9a-f]+|0o[0-7]+|0b[01]+|[0-9]+)\\b",
            "name": "constant.numeric.integer.eqasm"

        }
    ]
}