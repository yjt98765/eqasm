{
    "name": "eQASM",
    "displayName": "eQASM",
    "description": "eQASM language support for Visual Studio Code",
    "version": "0.1.0",
    "publisher": "eQASM-extension",
    "icon": "images/eqasm.jpg",
    "license": "EPL-1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/yjt98765/eqasm.git"
    },
    "bugs": {
        "url": "https://github.com/yjt98765/eqasm/issues"
    },
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:eqasm"
    ],
    "main": "out/extension",
    "contributes": {
        "languages": [{
            "id": "eqasm",
            "aliases": [
                "eqasm",
                "eQASM"
            ],
            "extensions": [
                ".eqasm",
                ".qisa"
            ],
            "configuration": "./mydsl.configuration.json"
        }],
        "grammars": [{
            "language": "eqasm",
            "scopeName": "text.eqasm",
            "path": "./syntaxes/mydsl.tmLanguage.json"
        }]
    },
    "devDependencies": {
        "vscode": "^1.1.33",
        "typescript": "^3.3.400",
        "@types/node": "^11.13.2"
    },
    "dependencies": {
        "vscode-languageclient": "^5.2.1"
    },
    "scripts": {
        "prepublish": "node ./node_modules/vscode/bin/install && tsc -p ./src",
        "compile": "tsc -p ./src",
        "watch": "tsc -w -p ./src",
        "update-vscode": "node ./node_modules/vscode/bin/install"
    }
}