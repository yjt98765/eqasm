{
    "name": "eQASM",
    "displayName": "eQASM",
    "description": "eQASM language support for Visual Studio Code",
    "version": "0.1.3",

    "publisher": "eQASM-extension",

    "publisher": "eQASM",

    "icon": "images/eqasm.jpg",
    "license": "EPL-1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/itemis/xtext-languageserver-example.git"
    },
    "bugs": {
        "url": "https://github.com/itemis/xtext-languageserver-example/issues"
    },
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:eqasm"
    ],
    "main": "out/extension",
    "contributes": {
        "languages": [{
            "id": "eqasm",
            "aliases": [
                "eqasm",
                "eQASM"
            ],
            "extensions": [
                ".eqasm",
                ".qisa"
            ],
            "configuration": "./mydsl.configuration.json"
        }],
        "grammars": [{
            "language": "eqasm",
            "scopeName": "text.eqasm",
            "path": "./syntaxes/mydsl.tmLanguage.json"
        }]
    },
    "devDependencies": {
        "vscode": "^1.1.33",
        "typescript": "^3.3.400",
        "@types/node": "^11.13.2"
    },
    "dependencies": {
        "vscode-languageclient": "^5.2.1"
    },
    "scripts": {
        "prepublish": "node ./node_modules/vscode/bin/install && tsc -p ./src",
        "compile": "tsc -p ./src",
        "watch": "tsc -w -p ./src",
        "update-vscode": "node ./node_modules/vscode/bin/install"
    },
    "__metadata": {
        "id": "85d3f714-27dc-4fe7-bdf3-f72e446005e9",
        "publisherDisplayName": "张晋荣",
        "publisherId": "a66fd1a4-a2c8-4b6a-af97-3a13d688bb4e"
    }
}
